{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/perplexity/(.*)",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}